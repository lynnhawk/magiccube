{
    "plugins": [
        "-tbfed-pagefooter",
        "github",
        "theme-api",
        "atoc",
        "emphasize",
        "katex",
        "include-codeblock",
        "page-footer"
    ],
    "pluginsConfig": {
        "-tbfed-pagefooter": {
            "copyright": "Copyright &copy www.pthink.com.cn 2016",
            "modify_label": "modified: ",
            "modify_format": "YYYY-MM-DD HH:mm:ss"
        },
        "github": {
            "url": "https://github.com/lynnhawk/magiccube.git"
        },
        "theme-api": {
            "languages": [
                {
                    "lang": "js",
                    "name": "JavaScript",
