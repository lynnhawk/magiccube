{
    "plugins": [
        "tbfed-pagefooter",
        "github",
        "theme-api",
        "atoc","emphasize","katex","include-codeblock"
    ],
    "pluginsConfig": {
        "fontSettings": {
            "theme": "sepia",
            "family": "serif",
            "size": 1
        },
        "theme-api": {
            "languages": [
                {
                    "lang": "js",
                    "name": "JavaScript",
                    "default": true
                },
                {
                    "lang": "java",
                    "name": "Java"
                }
            ]
        },
        "autocover": {
            "font": {
                "size": null,
                "family": "Impact",
                "color": "#FFF"
            },
            "size": {
                "w": 1800,
                "h": 2360
            },
            "background": {
                "color": "#09F"
            }
        },
        "github": {
            "url": "https://github.com/lynnhawk/magiccube.git"
        },
        "tbfed-pagefooter": {
            "copyright": "Copyright &copy www.pthink.com.cn 2016",
            "modify_label": "modified: ",
            "modify_format": "YYYY-MM-DD HH:mm:ss"
        },
        "atoc": { "addClass": true, "className": "atoc" }

    },
    "author": "phy",
    "pdf": {
        "pageNumbers": false,
        "margin": {
            "top": 36,
            "bottom": 36
        },
        "toc": true
    },
    "title": "魔方系统",
    "links": {
        "sidebar": {
            "新朋程": "http://www.pthink.com.cn"
        },
        "sharing": {
            "google": null,
            "facebook": null,
            "twitter": null,
            "weibo": null,
            "all": null
        }
    },
    "description": "魔方系统"
}