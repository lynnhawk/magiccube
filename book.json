{
    "plugins": [
        "tbfed-pagefooter",
        "github",
        "theme-api",
        "atoc",
        "emphasize",
        "katex",
        "include-codeblock"
    ],
    "pluginsConfig": {
        "fontSettings": {
            "theme": "sepia",
            "family": "serif",
            "size": 1
        },
        "theme-api": {
            "languages": [
                {
                    "lang": "js",
                    "name": "JavaScript",
                    "default": true
                },
                {
                    "lang": "java",
                    "name": "Java"
                }
            ]
        },
        "autocover": {
            "font": {
                "size": null,
                "family": "Impact",
                "color": "#FFF"
            },
            "size": {
                "w": 1800,
                "h": 2360
            },
            "background": {
                "color": "#09F"
            }
        },
        "github": {
            "url": "https://github.com/lynnhawk/magiccube.git"
        },
        "tbfed-pagefooter": {
            "copyright": "Copyright &copy www.pthink.com.cn 2016",
            "modify_label": "modified: ",
            "modify_format": "YYYY-MM-DD HH:mm:ss"
        },
        "atoc": { "addClass": true, "className": "atoc" },
        "sharing": { "weibo": false, "facebook": false, "twitter": false, "google": false, "instapaper": false, "vk": false, "all": [ "facebook", "google", "twitter", "weibo", "instapaper" ] }

    },
    "author": "phy",
    "pdf": {
        "pageNumbers": false,
        "margin": {
            "top": 36,
            "bottom": 36
        },
        "toc": true
    },
    "title": "魔方系统",
    "links": {
        "sidebar": {
            "新朋程": "http://www.pthink.com.cn"
        }
    },
    "description": "魔方系统"
}